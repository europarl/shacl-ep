shacl-ep:P13 owl:versionInfo "3.x.0";
  sh:path eli:work_type;
  sh:name "work type"@en;
  sh:nodeKind sh:IRI;
  sh:hasValue ep-document-types:PROCEDURE_RULES;
  sh:deactivated "true"^^xsd:boolean .

shacl-ep:Constraint-URI-identifier sh:select """PREFIX dcterms: <http://purl.org/dc/terms/>
SELECT $this ?localName ?identifier
WHERE {
 $this dcterms:identifier ?identifier .
 # extracts everything after last \"/\" from the URI
 BIND (REPLACE(STR($this), \"^.*/([^/]*)$\", \"$1\") as ?localName)
 FILTER(?localName != ?identifier)
}""";
  dcterms:description "The local part of the URI must be identical to the content of dcterms:identifier"@en;
  sh:message "The local part of the URI ({?localName}) is not identical to the value of dcterms:identifer ({?identifier})" .

shacl-ep:Constraint-URI-Expression-Work sh:select """PREFIX eli: <http://data.europa.eu/eli/ontology#>
SELECT $this ?workUri
WHERE {
 $this ^eli:is_realized_by ?work .
 BIND(STR(?work) AS ?workUri)
 FILTER(!STRSTARTS(STR($this), ?workUri))
}""";
  dcterms:description "The URI must start with the URI of the Work referencing this entity with eli:is_realized_by"@en;
  sh:message "The URI does not start with the URI of its Work ({?workUri})" .

shacl-ep:Constraint-URI-Manifestation-Expression sh:select """PREFIX eli: <http://data.europa.eu/eli/ontology#>
SELECT $this ?expressionUri
WHERE {
 $this ^eli:is_embodied_by ?expression .
 BIND(STR(?expression) AS ?expressionUri)
 FILTER(!STRSTARTS(STR($this), ?expressionUri))
}""";
  dcterms:description "The URI must start with the URI of the Expression referencing this entity with eli:is_embodied_by"@en;
  sh:message "The URI does not start with the URI of its Expression ({?expressionUri})" .
